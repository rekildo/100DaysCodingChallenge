// C++ program to find K'th Largest element

class Solution {
public:
    int findKthLargest(vector<int>& nums, int k) {
       // Sorts the array  
        int n = nums.size();
    sort(nums.begin(), nums.end());  
  
    // Reverses the array  
    reverse(nums.begin(),nums.end());  
  
    // Returns the required element  
    return nums[k-1]; 
    }
};
