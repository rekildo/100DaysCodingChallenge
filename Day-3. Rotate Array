#include<iostream>
using namespace std;
#include<vector>

void printArray(int a[],int size){
	for(int i = 0; i<size; i++){
		cout << a[i] << " ";
	}
	cout << "\n";
}

void rotateArray(int a[], int n, int d){
	int temp[n];
	for(int i = 0; i<n; i++){
		temp[i] = a[i];
	}//copied into temp array;

	int j = 0;
	for(int i = d; i<n; i++){
		a[j] = temp[i]; 
		j++;
	}//stored elements from d to n

	for(int i = 0; i<d; i++){
		a[j++] = temp[i];
	}
}

int main(){
	int a[] = {1,2,3,4,5};
	int size = sizeof(a)/sizeof(a[0]);
	cout << "Enter d : " << endl;
	int d;
	cin >> d;
	
	rotateArray(a,size,d);

	 printArray(a,size);
	return 0;
}
